import time as t
def temporizador(r):
    inicio= t.time()
    r=raw_input('ingresar')
    final=t.time()
    tiempo= round(final-inicio,0)
    if tiempo > 5.0 :
        print "sos una zorra"
        print tiempo
        return r
    else:
        print tiempo
        print "que rapida"
        return r

letra= ''

l= temporizador(letra)
print l.lower()
